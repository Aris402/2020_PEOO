using System;

class MainClass {
  public static void Main (string[] args) {
    int diametro = int.Parse(Console.ReadLine());
    string[] valores = Console.ReadLine().Split();
    int altura = int.Parse(valores[0]);
    int largura = int.Parse(valores[1]);
    int prof = int.Parse(valores[2]);
    if (diametro > altura || diametro > largura || diametro > prof)
        Console.WriteLine("N");
    else
        Console.WriteLine("S");
  }
}
