using System;

class MainClass {
  public static void Main (string[] args) {
    int a = 5000000, b = 7000000, anos = 0, ap = a * 3/100, bp = b * 2/100;
    while (b > a) {
      a += ap;
      b += bp;
      anos++;
    }
    Console.WriteLine("São necessários {0} anos para a população de A ser maior.", anos);
  }
}
