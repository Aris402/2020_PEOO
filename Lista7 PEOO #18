using System;

class MainClass {
  public static void Main (string[] args) {
    int a = 5000000, b = 7000000, anos = 0;
    while (b > a) {
      a += a * 3/100;
      b += b * 2/100;
      anos++;
    }
    Console.WriteLine("São necessários {0} anos para a população de A ser maior.", anos);
  }
}
