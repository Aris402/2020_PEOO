using System;

class MainClass {
  public static void Main (string[] args) {
    Console.WriteLine("Digite o valor gasto com o combustível em reais:");
    double gasto = double.Parse(Console.ReadLine());
    Console.WriteLine("Digite o valor do litro do combustível em reais:");
    double litro = double.Parse(Console.ReadLine());
    Console.WriteLine("Digite a distância percorrida em quilômetros");
    double km = double.Parse(Console.ReadLine());
    double media = (km/gasto) * litro/10;
    Console.WriteLine("O consumo médio é de {0:0.00} Km por litro", media);
    Console.WriteLine("O gasto por Km percorrido é R$ {0:0.00}", gasto/km/10);
  }
}
